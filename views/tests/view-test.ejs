        </div>


        <div class="container section-content">
            <!-- Row-1 information about client -->
            <div class="row">
                
                <div class="col-md-12">
                    <div class="form-group">
                         <label class="fw-bold"  for="subjectLine">2.1 Subject line (Enter Duration + Mode Of Test )</label>
                         <p><%= subjectLine %></p> 
                    </div>
                </div>

            </div>
            <!-- Row-1 Ends Here -->

            <div class="row">

                <div class="col-md-12">
                    <div class="form-group">
                         <label class="fw-bold"  for="testDetails">2.2 Test Details</label>
                        <p class="text-area-commentview">
                            <%= testDetails %>
                        </p>
                    </div>
                </div>

            </div>

            <div class="row">

                <div class="col-md-12">
                    <div class="form-group">
                         <label class="fw-bold"  for="Test Focus">2.3 Test Focus</label>
                        <p class="    ">
                            <%= testFocus %>
                        </p>
                    </div>
                </div>

            </div>

            <div class="row">

                <div class="col-md-12">
                    <div class="form-group">
