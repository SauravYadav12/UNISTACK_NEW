    <!-- Requirement ID Section -->
        <section class="heading mt-4 mb-4">

            <div class="container mx-auto" style="width: 500px; text-align: center">
                <h3 >Test ID# <em><%= testId %></em> </h3>
                <hr class="style-two" >
            </div>

  


    <!-- Form Section Starts here -->
    <form action="/tests/post-create-test" method="POST">    
    
      <!-- Form Button Section  -->
    <section class="func-buttons mt-4 mb-4">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group float-end">
                        <a href="/tests/update-test/<%= testId %>" class="btn btn-primary text-white mb-0 me-0" style="width: 100px">Edit</a>
                        <a href="/tests/test-dashboard/1" style="width: 100px" class="btn btn-danger text-white mb-0 me-0">Cancel</a>
                        <%if(username == 'Sam' || username == 'Anmol'){%>
                            <a href="/tests/delete-test/<%= testId %>" class="btn btn-warning text-white mb-0 me-0" style="width: 100px">Delete</a>
                        <%}%>
                    </div>
                </div>
            </div>
            <hr style="border: 1px solid #007BFF">
        </div>
    </section>
    <!-- Form Button Section Ends Here  -->

</section>
<!-- Requirement ID Section Ends Here -->

     <!-- Section 1 - contains information about Communication & rates -->
    <section class="interview-details mt-4 mb-4"> 
        <div class="container">
            <div  class="section-heading">
                <h3><i class="far fa-hand-point-down"></i> 1. Test Details </h3>
            </div>
            <hr class="style-two">
        </div>
        
        <div class="container section-content">
            
            <!-- Row 1 - Contains status and next Step -->
            <div class="row">
                <!-- column 1 -->
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="fw-bold"  for="date">1.1 Test Entered Date</label>
                        <p><%= testEnteredDate %></p> 
                    </div>
                </div>
                <!-- column 2 -->
                <div class="col-md-3">
                    <div class="form-group">
                         <label class="fw-bold"  for="testDuration">1.2 Test Duration</label>
                           <p><%= testDuration %></p> 
                    </div>    
                </div>
        
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="fw-bold"  for="date">1.1 Test Completion Date</label>
                        <p><%= testCompletionDate %></p> 
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                       <label class="fw-bold"  for="testStatus">1.4 Test Status</label>
                      <p>
                          <%= testStatus %>
                      </p> 
                    </div>
                </div>

            </div>
              <!-- Row 1 Ends Here -->

            <!-- Row 3 Marketing persons comments -->

            <div class="row">

                <div class="col-md-3">
                    <div class="form-group">
                         <label class="fw-bold"  for="consultant">1.5 Consultant</label>
                         <p><%= consultant %></p> 
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                         <label class="fw-bold"  for="marketing-person">1.6 Maketing Person</label>
                         <p><%= marketingPerson %></p> 
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                         <label class="fw-bold"  for="vendor-company">1.7 Vendor Company</label>
                         <p><%= vendorCompany %></p> 

                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                         <label class="fw-bold"  for="prime-vendor-company">1.8 Prime Vendor Company</label>
                         <p><%= primeVendorCompany %></p> 
                    </div>
                </div>

            </div>

            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                         <label class="fw-bold"  for="github">1.9 Submitted Test Via Github(if Yes Enter the Link)</label>
                         <p><%= gitHubLink %></p> 
                         
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                         <label class="fw-bold"  for="code">1.10 Submitted Test via other Medium(if Yes Enter the Link)</label>
                         <p><%= codeLink %></p> 
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                         <label class="fw-bold"  for="code">1.11 Result </label>
                       <p>
                           <%= result %>
                       </p>
                    </div>
                </div>
            </div>

        </div>
    </section>
    
    <!-- Section 1 Ends Here -->


    <!-- Section 2 -- Contain information about Clients -->

